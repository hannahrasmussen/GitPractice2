   "metadata": {},
   "outputs": [],
   "source": [
    "#surface area and volume of a cube\n",
    "\n",
    "def SA_cone(r,h):\n",
    "    return np.pi*r*(np.sqrt(r**2+h**2)+r)\n",
    "\n",
    "def vol_cone(r,h):\n",
    "    return (1/3)*h*np.pi*r**2"
   "id": "a8a66b45",
   "metadata": {},
   "outputs": [],
   "source": [
    "#surface area and volume of a sphere\n",
    "\n",
    "def SA_sphere(r):\n",
    "    return 4*np.pi*r**2\n",
    "\n",
    "def vol_sphere(r):\n",
    "    return (4/3)*np.pi*r**3"
   ]
  },
